#!/bin/bash
lfi-compile clang \
    -mllvm --aarch64-enable-compress-jump-tables=false \
    -mbranch-protection=bti \
    -mmark-bti-property \
    -flfi--gas \
    -ffixed-x18 -ffixed-x21 -ffixed-x22 -ffixed-x23 -ffixed-x24 -ffixed-x30 \
    "$@" \
    -fuse-ld=lld \
    -fPIC \
    -Wno-unused-command-line-argument
